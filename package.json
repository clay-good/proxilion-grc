{
  "name": "proxilion",
  "version": "1.0.0",
  "description": "Enterprise-grade AI Security Network Proxy for comprehensive monitoring, control, and protection of AI interactions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "npm run clean && tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js --platform=node --target=node18 --minify --sourcemap=linked --tree-shaking=true",
    "build:dev": "npm run clean && tsc && esbuild src/index.ts --bundle --format=esm --outfile=dist/index.js --platform=node --target=node18 --sourcemap=inline",
    "build:ui": "cd ui && npm install && npm run build",
    "build:all": "npm run build && npm run build:ui",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.md\" \"docs/**/*.md\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.md\" \"docs/**/*.md\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules ui/node_modules ui/.next",
    "ci": "npm run lint && npm run type-check && npm run test",
    "precommit": "npm run format && npm run lint:fix && npm run type-check",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "start:dev": "npm run build && npm start",
    "docker:build": "docker build -t proxilion:latest .",
    "docker:run": "docker run -p 8787:8787 proxilion:latest",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "validate": "npm run format:check && npm run lint && npm run type-check && npm run test"
  },
  "keywords": [
    "ai-security",
    "proxy",
    "llm-security",
    "data-loss-prevention",
    "dlp",
    "compliance",
    "enterprise",
    "openai",
    "anthropic",
    "google-ai",
    "cohere",
    "security-scanning",
    "pii-detection",
    "prompt-injection",
    "toxicity-detection",
    "gdpr",
    "hipaa",
    "pci-dss",
    "soc2",
    "cloudflare-workers",
    "edge-computing",
    "ai-governance",
    "llm-monitoring",
    "ai-observability"
  ],
  "author": "Proxilion Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proxilion/proxilion.git"
  },
  "bugs": {
    "url": "https://github.com/proxilion/proxilion/issues"
  },
  "homepage": "https://github.com/proxilion/proxilion#readme",
  "dependencies": {
    "@graphql-tools/schema": "^10.0.25",
    "@types/uuid": "^10.0.0",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-yoga": "^5.16.0",
    "hono": "^4.0.0",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240000.0",
    "@types/node": "^20.19.22",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "wrangler": "^3.28.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
